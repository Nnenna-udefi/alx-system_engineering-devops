[Unit]
Description=Gunicorn instance to serve Airbnb application
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/AirBnB_clone_v3/web_dynamic
ExecStart=/usr/local/bin/gunicorn --workers 3 --bind 0.0.0.0:5003 --access-logfile /tmp/airbnb-access.log --error-logfile /tmp/airbnb-error>

[Install]
WantedBy=multi-user.target
